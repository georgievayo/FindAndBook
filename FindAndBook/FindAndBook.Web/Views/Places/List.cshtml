@model ICollection<FindAndBook.Web.Models.Places.PlaceShortViewModel>
@{
    ViewBag.Title = "List";
}
<div class="col-md-10 col-md-offset-1">
    @if (Model.Count > 0)
    {
        foreach (var place in @Model)
        {
            @Html.Partial("_PlaceShort", place)
        }
    }
    else
    {
        <h2>There is no places to show.</h2>
    }
</div>